{
  "expo": {
    "name": "Golf-App",
    "slug": "Golf-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.reactjs.native.example.AllSquareReactNative",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.allsquarereactnative"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties", 
        {
        "ios": {
          "useFrameworks": "static"
        },
        "android": {
          "enableProguardInReleaseBuilds": true
        }

      }],
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "firebaseApiKey": "AIzaSyBlxvlcTeZ4GVywmVbFdn6r1AWySEkgBco",
      "firebaseAuthDomain": "reactnativegolfapp.firebaseapp.com",
      "firebaseProjectId": "reactnativegolfapp",
      "firebaseStorageBucket": "reactnativegolfapp.appspot.com",
      "firebaseMessagingSenderId": "466683770848",
      "firebaseAppId": "1:466683770848:web:91cc0133b6286202235d85"
    }
  }
}

